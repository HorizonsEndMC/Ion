{
	"extends": [
		"config:base"
	],
	"regexManagers": [
		{
			"fileMatch": ["(^|/)plugin.yml$"],
			"matchStrings": ["  - \"(?<depName>.*):(?<currentValue>.*)\""],
			"datasourceTemplate": "maven"
		}
	],
	"packageRules": [
	  {
		"matchUpdateTypes": ["major", "minor", "patch", "pin", "digest"],
		"automerge": true
	  }
	]
}
